
@{
    ViewData["Title"] = "Index";
}

@model List<CheeseMVC.Models.CheeseCategory>

<h2>Index</h2>

<table class="table">
    <tr>
        <th>CategoryID</th>
        <th>Name</th>
    </tr>
    @foreach(var category in @Model)
    {
        <tr>
            <td>@category.ID</td>
            <td>@category.Name</td>
        </tr>
    }
</table>
<p><a asp-controller="Category" asp-action="Add"> Add Category</a></p>

